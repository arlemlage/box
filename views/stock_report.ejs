<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('./partials/head'); -%>

</head>

<body>
    <!--*******************
        Preloader start
    ********************-->
    <div id="preloader">
        <div class="sk-three-bounce">
            <div class="sk-child sk-bounce1"></div>
            <div class="sk-child sk-bounce2"></div>
            <div class="sk-child sk-bounce3"></div>
        </div>
    </div>
    <!--*******************
        Preloader end
    ********************-->

    <!--**********************************
        Main wrapper start
    ***********************************-->
    <div id="main-wrapper">

        <!--**********************************
            Nav header start
        ***********************************-->
        <%- include('./partials/header_logo'); -%>

            <!--**********************************
            Nav header end
        ***********************************-->

            <!--**********************************
            Header start
        ***********************************-->
            <%- include('./partials/header',{titel: language.stock_report}); -%>

                <!--**********************************
            Header end ti-comment-alt
        ***********************************-->

                <!--**********************************
            Sidebar start
        ***********************************-->
                <%- include('./partials/sidebar'); -%>

                    <!--**********************************
            Sidebar end
        ***********************************-->

                    <div class="content-body">
                        <div class="container-fluid">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="card">
                                        <div class="card-header d-block">
                                            <h4 class="card-title"><%= language.product_stock_report %> </h4>
                                        </div>

                                        <form action="" method="post">

                                            <div class="card-body">
                                                <div class="row mb-3">
                                                    <div class="col-xl-6 col-sm-5">
                                                        <div class="form-group">
                                                            <label for="filter"><%= language.filter_by %> <span
                                                                    style="color: red;">*</span></label>
                                                            <select class="form-control" name="filter" id="stock_report_warehouse" onchange="myFunction">
                                                                <option value selected disabled>Select One</option>
                                                                <option id="warehouse" value="warehouse"><%= language.warehouse %></option>
                                                                <option id="product" value="product"><%= language.products %></option>
                                                            </select>
                                                        </div>
                                                    </div>

                                                    <div class="col-xl-6 col-sm-6">
                                                        <div class="form-group" id="render">
                                                            <label for="name"></label>
                                                            <select class="form-control mt-2" name="name" >
                                                                
                                                            </select>
                                                        </div>
                                                    </div>

                                                    <div class="col-xl-12 mt-3">
                                                        <div class="form-group">
                                                            <button type="button" class="btn btn-primary h-45 w-100" id="filter">
                                                                <i class="la la-filter"></i><%= language.filter %> 
                                                            </button>
                                                        </div>
                                                    </div>

                                                </div>
                                            </div>
                                        </form>

                                    </div>
                                </div>

                                <div class="col-lg-12" id="stock_report_table"> </div>

                            </div>
                        </div>
                    </div>

                    <!--**********************************
            Footer start
        ***********************************-->
                    <%- include('./partials/footer'); -%>

                        <!--**********************************
            Footer end
        ***********************************-->

    </div>
    <!--**********************************
        Main wrapper end
    ***********************************-->

    <!--**********************************
        Scripts
    ***********************************-->

    <%- include('./partials/script'); -%>

        <!--**********************************
        Scripts end
    ***********************************-->
</body>

</html>